<!DOCTYPE html>
<html lang="en" ng-app="routerApp">
<head>
    <meta charset="UTF-8">
    <link href="{{ asset('vendor/bootstrap/dist/css/bootstrap.min.css') }}" media="all" rel="stylesheet"
          type="text/css"/>
    <link href="{{ asset('asset/css/main.css') }}" media="all" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('asset/bower_components/angular-bootstrap/ui-bootstrap-csp.css') }}" media="all" rel="stylesheet" type="text/css"/>

    <script src="https://code.jquery.com/jquery-3.1.1.min.js"
            integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
            crossorigin="anonymous"></script>
    <script src="{{ asset('vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular.min.js"></script>
    <script src="//unpkg.com/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script src="{{ asset('asset/bower_components/angular-bootstrap/ui-bootstrap.js') }}"></script>
    <script src={{ asset('asset/bower_components/angular-animate/angular-animate.js') }}></script>
    <script src="{{ asset('asset/app/app.js') }}"></script>

    <title>{% block title %}Test Application{% endblock %}</title>
</head>
{% block body %}
<body>
<div class="container-fluid">
    <div class="row">
        {% block header %}{% endblock %}

        {% block left_aside %}{% endblock %}

        {% block content %}{% endblock %}

        {% block footer %}{% endblock %}
    </div>
</div>
{% endblock %}
</body>

</html>
